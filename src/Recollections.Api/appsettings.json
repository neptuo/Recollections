{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Identity": {
    "Password": {
      "RequireDigit": false,
      "RequiredLength": 4,
      "RequireLowercase": false,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": false
    }
  },
  "Cors": {
    "Origins": [ "http://localhost:33881", "https://app.recollections.neptuo.com", "https://recollections.app", "https://www.recollections.app" ]
  },
  "Accounts": {
    "Properties": {
      "Keys": [ "Map.PointOfInterest", "App.Theme" ]
    }
  },
  "ReverseProxy": {
    "Routes": {
      "maptiles" : {
        "ClusterId": "maptiles",
        "Match": {
          "Path": "/api/maptiles/{**catchAll}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api/maptiles" }
        ]
      }
    },
    "Clusters": {
      "maptiles": {
        "Destinations": {
          "maptiles/production": {
            "Address": "https://api.mapy.cz/v1/maptiles/"
          }
        }
      }
    }
  }
}
