{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@2.2.4",
      "destination": "wwwroot/lib/jquery"
    },
    {
      "provider": "jsdelivr",
      "library": "@fortawesome/fontawesome-free@6.5.2",
      "destination": "wwwroot/lib/fontawesome",
      "files": [
        "css/all.css",
        "webfonts/*"
      ]
    },
    {
      "provider": "unpkg",
      "library": "easymde@2.18.1-168.0",
      "destination": "wwwroot/lib/easymde",
      "files": [
        "dist/*"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "scss/**",
        "dist/js/bootstrap.min.js",
        "dist/js/bootstrap.min.js.map"
      ]
    },
    {
      "library": "popper.js@2.9.2",
      "destination": "wwwroot/lib/popper",
      "files": [
        "umd/*"
      ]
    }
  ]
}